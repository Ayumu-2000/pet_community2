<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <table class="table table-hover table-inverse">
        <thead>
          <tr>
            <th>[ID]</th>
            <th>会員</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td>:<%= user.name %></td>
            <td><%= link_to "会員の詳細", admin_user_path(user.id) %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>