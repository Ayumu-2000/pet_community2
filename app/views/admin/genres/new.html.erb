
<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <h4>ジャンル新規作成</h4>
      <%= form_with model: @genre, url: admin_genres_path, method: :post do |f| %>
        <h4>ジャンル名</h4>
        <%= f.text_field :genre_name %><br>
        <%= f.submit "登録する" %>
      <% end %>
    </div>
  </div>
</div>
